<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane stylesheets="@../styles/newGameStyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.hr.kcosic.project.controllers.NewGameController">
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="300.0" BorderPane.alignment="CENTER">
         <Button fx:id="btnHotSeatGame" mnemonicParsing="false" onAction="#newHotSeatGame" text="Hot seat game">
            <VBox.margin>
               <Insets bottom="30.0" top="30.0"/>
            </VBox.margin>
         </Button>
         <Button fx:id="btnNetworkGame" mnemonicParsing="false" onAction="#newNetworkGame" text="Network game">
            <VBox.margin>
               <Insets bottom="30.0" top="30.0"/>
            </VBox.margin>
         </Button>
         <Button fx:id="btnBack" mnemonicParsing="false" onAction="#back" text="Back">
            <VBox.margin>
               <Insets bottom="30.0" top="30.0"/>
            </VBox.margin>
         </Button>
      </VBox>
   </center>
</BorderPane>
